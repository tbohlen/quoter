{
    "name": "Internet Research Mapping",
    "description": "Tool for building Beagle Maps based on research you do",
    "version": "0.1",
    "manifest_version": 3,
    "action": {
        "default_icon": {
            "16": "icon.png",
            "32": "icon.png"
        },
		"default_popup": "popup.html"
    },
    "icons": {
        "16": "icon.png",
        "32": "icon.png"
    },
    "permissions": [
        "contextMenus",
        "storage"
    ],
    "background": {
        "service_worker": "quoteMaker.js"
    }
}
